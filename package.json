{
  "name": "doc-link-checker-cli",
  "version": "1.0.4",
  "description": "A CLI wrapper around doc-link-checker so you can verify documentation links in a script.",
  "license": "GPLv3",
  "type": "module",
  "packageManager": "yarn@3.3.0",
  "dependencies": {
    "clipanion": "3.2.0-rc.14",
    "doc-link-checker": "1.1.0",
    "typanion": "3.12.1"
  },
  "devDependencies": {
    "@types/node": "16.18.3",
    "prettier": "2.7.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.4",
    "xo": "0.52.4"
  },
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "doc-link-checker": "./index.js"
  },
  "scripts": {
    "build": "tsc --declaration --noEmitOnError",
    "lint": "xo",
    "reformat": "prettier -w src"
  },
  "files": [
    "index.js",
    "src/**/*.ts",
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "tsconfig.json"
  ],
  "sideEffects": false,
  "homepage": "https://github.com/djmattyg007/doc-link-checker-cli",
  "repository": {
    "type": "git",
    "url": "https://github.com/djmattyg007/doc-link-checker-cli.git"
  },
  "keywords": [
    "link checker",
    "links",
    "linter",
    "linting",
    "markdown",
    "documentation"
  ]
}
